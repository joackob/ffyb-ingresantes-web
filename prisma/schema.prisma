// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Carreras {
 id Int @id @default(autoincrement())
 Carrera String
}
model Asignaturas {
 id Int @id @default(autoincrement())
 Asignatura String
 Carrera_id Int
}
